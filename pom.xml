			<version>4.1.2</version>
		</dependency>
		<dependency>
			<groupId>org.apache.poi</groupId>
			<artifactId>poi-ooxml</artifactId>
			<version>4.1.2</version>
		</dependency>
		<dependency>
      	  	<groupId>xerces</groupId>
       		<artifactId>xerces</artifactId>
        	<version>2.4.0</version>
    	</dependency>
    	<dependency>
       		<groupId>xerces</groupId>
        	<artifactId>xercesImpl</artifactId>
